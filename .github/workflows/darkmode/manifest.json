{
    "manifest_version": 2,
    "name": "Discord Void theme override",
    "version": "1.2",
    "description": "Set text color to white and background color to black.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["https://discord.com/*"],
        "css": ["style.css"]
      }
    ]
  }
  